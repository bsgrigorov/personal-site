{
  "name": "personal-site",
  "version": "4.0.0",
  "private": true,
  "description": "Borislav Grigorov's personal website and resume",
  "homepage": "https://bgrigorov.com/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bsgrigorov/personal-site.git"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.26.1",
  "scripts": {
    "analyze": "pnpm build && source-map-explorer build/static/js/*.chunk.js",
    "build": "rimraf ./build && react-scripts build",
    "lint": "eslint \"**/*.js\"",
    "lint:fix": "eslint \"**/*.js\" --fix",
    "predeploy": "pnpm run build && react-snap",
    "start": "react-scripts start",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "dayjs": "^1.11.19",
    "markdown-to-jsx": "^7.7.17",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-burger-menu": "^3.1.0",
    "react-dom": "^18.3.1",
    "react-ga": "^3.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.30.2",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "^7.37.5",
    "react-snap": "^1.23.0",
    "rimraf": "^5.0.10",
    "sass": "1.79.5",
    "sass-migrator": "^2.5.6",
    "source-map-explorer": "^2.5.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
